import sys

def tonum(s: str):
    try:
        return int(s)
    except ValueError:
        return float(s)

def main() -> int:
    lines = sys.stdin.read().splitlines()
    if not lines:
        return 1
    total = 0.0
    for line in lines:
        line = line.strip()
        if line == "":
            return 1
        try:
            total += tonum(line)
        except ValueError:
            return 1
    print(f"{total:.1f}")
    return 0

if __name__ == "__main__":
    raise SystemExit(main())
